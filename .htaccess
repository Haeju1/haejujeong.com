# mod_rewrite starts here

RewriteEngine on

# Does not appy to existing directories, meaning that if the folder exists on the server, then don't change anything and don't run the rule!

RewriteCodn %{REQUET_FILENAME} !-d

# Check for file in directory with .html extension

RewriteCond %{REQUET_FILENAME}\.html -f

# Here we show the page taht has .html extension

RewriteRule ^(.*)$ $1.html [NC,L]